        System.out.println(man2.toString());
        manusia man3 = new manusia("Kendall Jenner", "123456789", false, true);
        System.out.println(man3.toString());
        System.out.println("\n========== DATA MAHASISWA ==========");
        mahasiswaFilkom mhs1 = new mahasiswaFilkom("I Putu Yoga", "351923848239", false, false, "205150601111018", 3.7);
