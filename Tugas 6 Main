        System.out.println(mhs2.toString());
        mahasiswaFilkom mhs3 = new mahasiswaFilkom("Esy Mira", "876543219", false, true, "215150701111034", 2.5);
        System.out.println(mhs3.toString());
        System.out.println("\n=========== DATA PEKERJA ===========");
        pekerja pekerja1 = new pekerja("Iqbal Biondy", "351717969","195102439283", true, true, 7, 22);
        System.out.println(pekerja1.toString());
